<!-- OpenCV 滤镜测试列表 -->
<filters>

    <!-- 自适应二值化 -->
    <opencv_auto_binary max="255.0" delta="10.0" type="0" size="65" />

    <!-- 均值模糊滤镜 -->
    <?opencv_box_blur ksize_x="9" ksize_y="9" />

    <!-- 高斯模糊滤镜 -->
    <?opencv_gauss_blur ksize_x="9" ksize_y="9" sigma_x="0.0" sigma_y="0.0" />

    <!-- Hough 圆搜索滤镜 -->
    <?opencv_hough_circles dp="2.0" min_dist="120.0"
        param1="200.0" param2="100.0" min_radius="0" max_radius="240" />

    <!-- Hough 线搜索滤镜 -->
    <?opencv_hough_lines ksize="3" param1="50.0" param2="200.0"
        thres="100" rho="1.0" theta="0.01745" srn="0.0" stn="0.0" />

    <!-- Hough 线搜索滤镜 -->
    <?opencv_hough_linesp ksize="3" param1="50.0" param2="200.0"
        thres="80" rho="1.0" theta="0.01745" min_len="30.0" max_gap="10.0" />

    <!-- 中值模糊滤镜 -->
    <?opencv_median_blur ksize="5" />

    <!-- QrCode 识别滤镜 -->
    <?opencv_qrcode_decode size="25" delta="10" />

</filters>
